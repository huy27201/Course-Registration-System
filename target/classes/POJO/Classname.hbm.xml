<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.Classname" table="classname" schema="schoolmanagement">
        <id name="id">
            <column name="ID" sql-type="char(6)" length="6"/>
        </id>
        <property name="total">
            <column name="Total" sql-type="int" not-null="true"/>
        </property>
        <property name="maleCount">
            <column name="MaleCount" sql-type="int" not-null="true"/>
        </property>
        <property name="femaleCount">
            <column name="FemaleCount" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>