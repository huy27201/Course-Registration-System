<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.main.POJO.Sinhvien" table="sinhvien" schema="schoolmanagement">
        <id name="id">
            <column name="ID" sql-type="char(8)" length="8"/>
        </id>
        <property name="ho">
            <column name="Ho" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="ten">
            <column name="Ten" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="ngaySinh">
            <column name="NgaySinh" sql-type="date" not-null="true"/>
        </property>
        <property name="gioiTinh">
            <column name="GioiTinh" sql-type="char(3)" length="3" not-null="true"/>
        </property>
        <property name="lop">
            <column name="Lop" sql-type="char(6)" length="6" not-null="true"/>
        </property>
        <property name="taiKhoan">
            <column name="TaiKhoan" sql-type="char(8)" length="8" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>